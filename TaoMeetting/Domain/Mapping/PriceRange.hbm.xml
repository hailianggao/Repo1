<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Domain" namespace="TaoMeetting.Domain">
  <class name="PriceRange" table="PriceRange" lazy="true">
    <id name="id" column="id" type="int">
      <generator class="native" />
    </id>
    <property type="float"  name="lowprice" column="lowprice" not-null="true"/>
    <property type="float" name="highprice" column="highprice" not-null="true"/>
  </class>
</hibernate-mapping>